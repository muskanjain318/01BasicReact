import React, { useEffect, useState } from 'react';

function Products() {
  const [product, setProduct] = useState([]);
  useEffect(() => {
    fetch('https://fakestoreapi.com/products')
      .then((res) => res.json())
      .then((data) => setProduct(data));
  }, []);
  return (
    <>
      <h1> hey </h1>,
      <ul>
        {product.map((p) => {
          <li key={p.id}>{p.title}</li>;
        })}
      </ul>
    </>
  );
}

export default Products;
